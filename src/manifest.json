{
  "name": "Edficiency+",
  "description": "Unlock Edficiency's maximum potential",
  "version": "1.0",
  "permissions": [
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://westwood.edf.school/*"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [ "https://westwood.edf.school/*" ],
      "js": [ "injected-script.js" ],
      "css": [ "bootstrap.min.css" ],
      "world": "MAIN",
      "run_at": "document_end"
    }
  ]
}
